{
    "name": "andrewdyer/slim3-validator",
    "description": "A validation library for the Slim Framework built using Respect/Validation",
    "type": "library",
    "keywords": ["slim", "slim3", "slim-framework"],
    "homepage": "https://github.com/andrewdyer/slim3-validator",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Dyer",
            "homepage": "https://andrewdyer.rocks",
            "role": "developer"
        }
    ],
    "support": {
        "source": "https://github.com/andrewdyer/slim3-validator.git"
    },
    "require": {
        "php": ">=7.0.0",
        "slim/slim": "^3.11.0",
        "respect/validation": "^1.1.29"
    },
    "autoload": {
